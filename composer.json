       "Laravel",
       "Voyager",
       "admin"
     ],
    "require": {
        "tcg/voyager": "dev-1.6-l10",
        "spatie/laravel-medialibrary": "^10.0.0",
        "illuminate/support": "~8.0|~9.0|~10.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "laravel/framework": "~8.0|~9.0|~10.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Lamtin222\\VoyagerExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lamtin222\\VoyagerExtension\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lamtin222\\VoyagerExtension\\VoyagerExtensionServiceProvider"
            ]
        }
    }
